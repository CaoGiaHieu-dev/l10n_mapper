{
	"@@locale": "en",
	"application_name": "Localization mapper",
	"@application_name": {
		"type": "text"
	},
	"deposit_timeframe": "Instant",
	"@deposit_timeframe": {
		"type": "text"
	},
	"balance_reverted": "{currency} balance",
	"@balance_reverted": {
		"type": "text",
		"placeholders": {
			"currency": {}
		}
	},
	"convert_before_withdraw": "* For withdrawing your {convertFrom} you first need to convert it back to {convertTo}",
	"@convert_before_withdraw": {
		"type": "text",
		"placeholders": {
			"convertFrom": {},
			"convertTo": {}
		}
	},
	"convert_before_withdraw_again": "* For withdrawing your {convertFrom} you first need to convert it back to {convertTo}",
	"@convert_before_withdraw_again": {
		"type": "text",
		"placeholders": {
			"convertFrom": {},
			"convertTo": {}
		}
	}
}